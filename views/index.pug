extends layout

block content
  h1= title
  p This is my second node project #{title}
  button(type="button", class="btn btn-primary" onclick="update()") Press Me for the contents
  div(id="target" style="margin-top:10px") Display
  a(href="/georgia" style="margin-top:10px") Georgia

  script.
    let client_count = 0;
    function update(){
      client_count++;
      document.getElementById('target').innerHTML = `Value now ${client_count}`;
    }
